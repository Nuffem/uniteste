uniteste # ./uniteste.0

uniteste.principal([
  uniteste.descrever("Aritmética" [
    uniteste.descrever("Adição" [
      uniteste.iguais((1 + 1) 2)
      uniteste.iguais((2 + 3) 5)
      uniteste.iguais((4 + 8) 12)
    ])
    uniteste.descrever("Subtração" [
      uniteste.iguais((5 - 3) 2)
      uniteste.iguais((8 - 2) 6)
    ])
  ])
  uniteste.descrever("Operadores lógicos" [
    uniteste.descrever("Não" [
      uniteste.iguais((! 0) 1)
      uniteste.iguais((! 1) 0)
    ])
  ])
])